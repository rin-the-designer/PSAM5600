{
  "builds": [
    {
      "src": "assignment-3/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/assignment-3/(.*)",
      "dest": "assignment-3/main.py"
    },
    {
      "src": "/assignment-3",
      "dest": "assignment-3/main.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
